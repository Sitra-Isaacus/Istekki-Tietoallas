{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "NetworkResourceGroup": {
      "value": "{{ network_res_group }}"
    },
    "SubnetCIDR": {
      "value": "{{ subnet_cidr }}"
     },
    "IntegrationSubnetCIDR": {
      "value": "{{ integration_subnet_cidr }}"
    },
    "DBLogin": {
      "value": "{{ db_admin }}"
    },
    "DBPwd": {
      "value": "{{ db_admin_pwd }}"
    },
    "AdminLogin": {
      "value": "{{ admin_name }}"
    },
    "AdminPassword": {
      "value": "not-like-this"
    },
    "AdminSSHKey": {
      "value": "{{ admin_ssh_key }}"
    },
    "NetworkName": {
      "value": "{{ vnet_name }}"
    },
    "ADLSKeyVersion": {
      "value": "{{ adls_key_version }}"
    }
  }
}
